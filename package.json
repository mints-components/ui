{
  "name": "@mints/ui",
  "packageManager": "yarn@4.7.0",
  "version": "1.0.0-alpha.21",
  "type": "module",
  "files": [
    "dist",
    "types"
  ],
  "types": "types",
  "main": "./dist/mints-ui.umd.cjs",
  "module": "./dist/mints-ui.js",
  "exports": {
    ".": {
      "import": {
        "types": "./types/index.d.ts",
        "default": "./dist/mints-ui.js"
      },
      "require": {
        "types": "./types/index.d.ts",
        "default": "./dist/mints-ui.umd.cjs"
      }
    },
    "./style.css": "./dist/mints-ui.min.css"
  },
  "scripts": {
    "clean": "rm -rf dist && rm -rf types",
    "build": "yarn clean && tsc -p tsconfig.build.json && vite build",
    "lint": "eslint . --fix",
    "prettier": "prettier . --write",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "deploy-storybook": "rm -rf storybook-static && storybook build && gh-pages -d storybook-static",
    "prepublish": "yarn build",
    "publish": "yarn npm publish --access public",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{html,json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "framer-motion": "^12.15.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@mints/eslint-config": "^2.0.0",
    "@storybook/addon-docs": "^9.0.13",
    "@storybook/addon-links": "^9.0.13",
    "@storybook/addon-onboarding": "^9.0.13",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react-vite": "^9.0.13",
    "@tailwindcss/cli": "^4.1.4",
    "@tailwindcss/vite": "^4.1.3",
    "@types/gh-pages": "^6",
    "@types/node": "^20.4.6",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.22.0",
    "eslint-plugin-storybook": "^9.0.13",
    "gh-pages": "^6.3.0",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "pinst": "^3.0.0",
    "prettier": "^3.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "storybook": "^9.0.13",
    "tailwindcss": "^4.1.3",
    "typescript": "^5.8.3",
    "vite": "^7.0.0"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.1.3"
  }
}
